<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite verbose="1" name="Playwright Parallel Tests" annotations="JDK" parallel="methods" thread-count="2">

    <listeners>
        <listener class-name="configuration.listeners.AnnotationTransformer"/>
        <listener class-name="com.epam.reportportal.testng.ReportPortalTestNGListener"/>
    </listeners>

    <test name="PlaywrightParallelTest">
        <classes>
            <class name="tests.ui.webstudio.studio_smoke.TestPlaywrightAdminEmail"/>
            <class name="tests.ui.webstudio.studio_issues.TestPlaywrightAddProperty"/>
        </classes>
    </test>

</suite>