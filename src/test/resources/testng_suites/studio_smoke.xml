<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite verbose="1" name="WebStudio Smoke Tests" annotations="JDK" parallel="tests" thread-count="3">

    <listeners>
        <listener class-name="configuration.listeners.AnnotationTransformer"/>
        <listener class-name="configuration.listeners.RetryAnalyzer"/>
        <listener class-name="com.epam.reportportal.testng.ReportPortalTestNGListener"/>
    </listeners>

    <test name="StudioSmoke.TestAdminEmail"><classes><class name="tests.ui.webstudio.studio_smoke.TestAdminEmail"/></classes></test>
    <test name="StudioSmoke.TestPlaywrightUserSettingsAndDetails"><classes><class name="tests.ui.webstudio.studio_smoke.TestPlaywrightUserSettingsAndDetails"/></classes></test>

</suite>