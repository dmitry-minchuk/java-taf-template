<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite verbose="1" name="Sample-UI-tests" annotations="JDK" parallel="tests" thread-count="3">

    <listeners>
        <listener class-name="configuration.listeners.AnnotationTransformer"/>
        <listener class-name="com.epam.reportportal.testng.ReportPortalTestNGListener"/>
    </listeners>

    <test name="StudioIssues.TestAddProperty"><classes><class name="tests.ui.webstudio.studio_issues.TestAddProperty"/></classes></test>
    <test name="StudioIssues.TestWithJiraTicket"><classes><class name="tests.ui.webstudio.studio_issues.TestAddPropertyExtraStateAppears"/></classes></test>
    <test name="StudioIssues.TestAddModuleWithPathStar"><classes><class name="tests.ui.webstudio.studio_issues.TestAddModuleWithPathStar"/></classes></test>
    <test name="StudioIssues.TestAddModuleWithPathExistingModule"><classes><class name="tests.ui.webstudio.studio_issues.TestAddModuleWithPathExistingModule"/></classes></test>
    <test name="StudioIssues.TestAddElementToCollectionSet"><classes><class name="tests.ui.webstudio.studio_issues.TestAddElementToCollectionSet"/></classes></test>
    <test name="StudioIssues.TestAddDeleteRowWithoutSaving"><classes><class name="tests.ui.webstudio.studio_issues.TestAddDeleteRowWithoutSaving"/></classes></test>
    <test name="StudioIssues.TestAddDeleteDesignRepository"><classes><class name="tests.ui.webstudio.studio_issues.TestAddDeleteDesignRepository"/></classes></test>
</suite>